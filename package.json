{
  "name": "@fleek-platform/functions-esbuild-config",
  "version": "0.0.17",
  "description": "ESBuild config for Fleek Functions",
  "module": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "https://github.com/fleek-platform/functions-esbuild-config",
  "homepage": "https://github.com/fleek-platform/functions-esbuild-config",
  "license": "MIT",
  "keywords": [
    "fleek",
    "esbuild",
    "config",
    "typescript",
    "serverless",
    "lambda",
    "fleek-functions",
    "edge"
  ],
  "type": "module",
  "scripts": {
    "build": "tsc --build",
    "build:ci": "pnpm run build",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "prepare": "husky"
  },
  "lint-staged": {
    "*": [
      "prettier . --check",
      "eslint"
    ]
  },
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "esbuild": ">=0.21.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.2.0",
    "@types/node": "^22.5.4",
    "esbuild": "^0.21.4",
    "eslint": "^9.2.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  }
