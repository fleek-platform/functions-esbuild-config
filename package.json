{
  "name": "@fleek-platform/functions-esbuild-config",
  "version": "1.0.0",
  "description": "ESBuild config for Fleek Functions",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "test": "jest --passWithNoTests",
    "prepare": "husky"
  },
  "keywords": [
    "fleek",
    "esbuild",
    "config",
    "typescript",
    "serverless",
    "lambda",
    "fleek-functions",
    "edge"
  ],
  "lint-staged": {
    "*": [
      "prettier . --check",
      "eslint"
    ]
  },
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "dependencies": {
    "esbuild": "^0.23.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.2.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.11",
    "@types/semver": "^7.5.8",
    "eslint": "^9.2.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "8.5.0"
  }
}
